{
  "expo": {
    "name": "kitabak",
    "slug": "kitabak",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/photo_2025-03-22_05-01-06-removebg-preview.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "bundleIdentifier": "com.marikoo7.kitabak",
      "supportsTablet": true
    },
    "android": {
      "package": "com.marikoo7.kitabak",
      "versionCode": 1,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/photo_2025-03-22_05-01-06.jpg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/photo_2025-03-22_05-01-06-removebg-preview.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1416cce1-89c0-42e3-a420-d70dee8493b6"
      }
    }
  }
}

